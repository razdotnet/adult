<%= form_for @user, url: update_account_path do |f| %>
	<%= f.label :email %>
	<%= f.text_field :email %>

	<%= f.label :username %>
	<%= f.text_field :username %>

	<%= f.label :password %>
	<%= f.password_field :password %>

	<%= f.label :model %>
	<%= f.check_box :model %>
	<br>
	<%= f.submit %>
<% end %>


